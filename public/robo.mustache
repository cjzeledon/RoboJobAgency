<!DOCTYPE html>
<html>
<head>
  <title>
    Robo Staffing Agency
  </title>
  <link rel="stylesheet" type="text/css" href="./styles.css">
</head>
<body>

  {{#users}}
  {{#job}}
    <div class="profile">
    <img src="{{avatar}}">
    <p class="robo_name">{{name}}</p>
    <p class="robo_jobtitle">{{job}}</p>
    <p class="robo_company">{{company}}</p>
    </div>
    {{/job}}
    {{^job}}
      <div class="profile">
      <img src="{{avatar}}">
      <p class="robo_name">{{name}}</p>
      <p class="available">Need Work</p>
      </div>
      {{/job}}
  {{/users}}

</body>

</html>
